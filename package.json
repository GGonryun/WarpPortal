{
  "name": "@warpportal/source",
  "version": "0.0.0",
  "license": "MIT",
  "prisma": {
    "schema": "./packages/prisma/schema.prisma"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "services:start": "docker-compose up -d",
    "web": "npx nx run web:dev",
    "link": "npx nx run-many --target=build --all",
    "tunnel:redis": "ngrok tcp 6379",
    "tunnel:prontera": "lt --port 3333",
    "redis": "npx redis-cli",
    "prisma:studio:local": "dotenv -e ./packages/prisma/.env -- npx prisma studio",
    "prisma:generate:local": "dotenv -e ./packages/prisma/.env -- npx prisma generate",
    "prisma:migrate:local": "dotenv -e ./packages/prisma/.env -- npx prisma migrate dev",
    "prisma:reset:local": "dotenv -e ./packages/prisma/.env -- npx prisma migrate reset",
    "prisma:push:local": "dotenv -e ./packages/prisma/.env -- npx prisma db push"
  },
  "private": true,
