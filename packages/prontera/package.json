{
  "name": "@warpportal/prontera",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "prontera",
    "projectType": "application",
    "sourceRoot": "packages/prontera/src",
    "targets": {
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "prontera:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "prontera:build:development"
          },
          "production": {
            "buildTarget": "prontera:build:production"
          }
        }
      }
    }
  }
}
